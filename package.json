{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "12.13.0"
  },
  "scripts": {
    "start": "node ./dist/bin/www",
    "build": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "dev": "nodemon --exec babel-node ./src/bin/www",
    "prod": "npm run clean && npm run build && npm start"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "~4.16.1",
    "mongoose": "^5.9.9",
    "morgan": "~1.9.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
